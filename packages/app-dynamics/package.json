{
  "name": "@skhye05/app-dynamics",
  "version": "1.0.3",
  "description": "Add a plugin description",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "6.0.0",
      "android": "6.0.0"
    },
    "hooks": [
      {
        "name": "app-dynamics",
        "type": "before-checkForChanges",
        "script": "hooks/before-checkForChanges.js",
        "inject": true
      },
      {
        "name": "app-dynamics",
        "type": "before-watchPatterns",
        "script": "hooks/before-watchPatterns.js",
        "inject": true
      }
    ]
  },
  "scripts": {
    "preuninstall": "node preuninstall",
    "postinstall": "node postinstall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skhye05/ns-plugins.git"
  },
  "keywords": [
    "NativeScript",
